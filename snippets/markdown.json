{
  "code block": {
    "prefix": ["code"],
    "body": ["```$1\n```"],
    "description": "create code block"
  },
  "< symbol": {
    "prefix": ["<"],
    "body": ["&lt;"],
    "description": "replace < to &lt;"
  },
  "> symbol": {
    "prefix": [">"],
    "body": ["&gt;"],
    "description": "replace > to &gt;"
  },
  "| symbol": {
    "prefix": ["|"],
    "body": ["&#124;"],
    "description": "replace | to &#124;"
  },
  "row": {
    "prefix": ["row"],
    "body": ["|$1|$2|$3|"],
    "description": "generate table row"
  },
  "leq": {
    "prefix": ["leq"],
    "body": ["\\leq"],
    "description": "Less than or equal to"
  },
  "geq": {
    "prefix": ["geq"],
    "body": ["\\geq"],
    "description": "Greater than or equal to"
  },
  "neq": {
    "prefix": ["neq"],
    "body": ["$1 \\neq $2"],
    "description": "Not equal"
  },
  "approx": {
    "prefix": ["approx"],
    "body": ["$1 â‰ˆ $2"],
    "description": "Approx"
  },
  "pm": {
    "prefix": ["pm"],
    "body": ["\\pm"],
    "description": "Plus or minus"
  },
  "abs": {
    "prefix": ["abs"],
    "body": ["|$1|"],
    "description": "Absolute value"
  },
  "frac": {
    "prefix": ["frac"],
    "body": ["\\frac{$1}{$2}"],
    "description": "Fractional expressions"
  },
  "underline": {
    "prefix": ["underline"],
    "body": ["\\underline{$1}"],
    "description": "Underline"
  },
  "to": {
    "prefix": ["to"],
    "body": ["$1 \\to $2"],
    "description": "From X to Y"
  },
  "space": {
    "prefix": ["space"],
    "body": ["\\,"],
    "description": "Space in latex"
  },
  "times": {
    "prefix": ["times"],
    "body": ["\\times"],
    "description": "Multiplication in latex"
  },
  "cdot": {
    "prefix": "cdot",
    "body": ["\\cdot"],
    "description": "Multiplication(dot indication) in latex"
  },
  "div": {
    "prefix": ["div"],
    "body": ["\\div"],
    "description": "Division in latex"
  },
  "because": {
    "prefix": "because",
    "body": ["\\because"],
    "description": "Because in latex"
  },
  "therefore": {
    "prefix": "therefore",
    "body": ["\\therefore"],
    "description": "Therefore in latex"
  }
}
